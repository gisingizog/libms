{
    "swagger": "2.0",
    "info": {
        "description": "STARTER TEMPLATE",
        "title": "Simple student crud",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Irizapeace",
            "email": "irizapeace@gmail.com"
        },
        "version": "1.0.0"
    },
    "tags": [
        {
            "name": "STUDENT OPERATIONS",
            "description": "Operations on students"
        },
        {
            "name": "BOOK OPERATIONS",
            "description": "Operations on books"
        } 
    ],
    "security": [
        {
            "Bearer": []
        }
    ],
    "securityDefinitions": {
        "Bearer": {
            "type": "apiKey",
            "name": "auth-token",
            "in": "header",
            "bearerFormat": "JWT",
            "scheme": "bearer"
        }
    },
    "paths": {
        "/student/signIn": {
            "post": {
                "tags": [
                    "USER OPERATIONS"
                ],
                "summary": "Login",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "type": "object",
                        "consumes": "application/json",
                        "properties": {
                            "email": {
                                "type": "string",
                                "required": "true"
                            },
                            "password": {
                                "type": "string",
                                "required": "true"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Invalid data"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/student/admin/register": {
            "post": {
                "tags": [
                    "USER OPERATIONS"
                ],
                "summary": "Register",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "type": "object",
                        "consumes": "application/json",
                        "properties": {
                            "firstname": {
                                "type": "string",
                                "required": "true"
                            },
                            "lastname": {
                                "type": "string",
                                "required": "true"
                            },
                            "phone": {
                                "type": "string",
                                "required": "true"
                            },
                            "email": {
                                "type": "string",
                                "required": "true"
                            },
                            
                            "password": {
                                "type": "string",
                                "required": "true"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Invalid data"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/student/profile": {
            "get": {
                "tags": [
                    "USER OPERATIONS"
                ],
                "summary": "Get user profile",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Invalid data"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        
        "/book/all": {
            "get": {
                "tags": [
                    "BOOK OPERATIONS"
                ],
                "summary": "Get All  Book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Invalid data"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/book/create": {
            "post": {
                "tags": [
                    "OWNER OPERATIONS"
                ],
                "summary": "Register a book",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "type": "object",
                        "consumes": "application/json",
                        "properties": {
                            "firstname": {
                                "type": "string",
                                "required": "true"
                            },
                            "lastname": {
                                "type": "string",
                                "required": "true"
                            },
                            "phone": {
                                "type": "string",
                                "required": "true"
                            },
                            "address": {
                                "type": "string",
                                "required": "true"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Invalid data"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        }
    }
}